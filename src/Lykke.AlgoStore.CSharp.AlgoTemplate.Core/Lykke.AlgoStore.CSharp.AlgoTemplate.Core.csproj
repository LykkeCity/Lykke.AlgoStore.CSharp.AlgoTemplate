<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>Lykke.AlgoStore.CSharp.AlgoTemplate.Core</AssemblyName>
    <RootNamespace>Lykke.AlgoStore.CSharp.AlgoTemplate.Core</RootNamespace>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Domain\Entities\AlgoClientInstanceData.cs" />
    <Compile Remove="Domain\Entities\BaseAlgoData.cs" />
    <Compile Remove="Domain\Entities\BaseAlgoInstance.cs" />
    <Compile Remove="Domain\Entities\BaseValidatableData.cs" />
